<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
	<Version>1.0.0</Version>
	<AssemblyVersion>$(Version)</AssemblyVersion>
	<FileVersion>$(Version)</FileVersion>
	<GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

	<Authors>phatvphat</Authors>
	<Company>phatvphat</Company>
	<Description>A tool to clean up old versions of applications and temporary files on your PC.</Description>
	<Copyright>Copyright (c) 2025 phatvphat</Copyright>
	<Product>CleanUpThisPc</Product>
	<AssemblyTitle>CleanUpThisPc</AssemblyTitle>
  </PropertyGroup>

</Project>
